  "reporting": {
    "slack_url": "https://slack-reporting-url",
    "service_url": "https://powerloom-reporting-url"
  },
  "logs": {
    "trace_enabled": false,
    "write_to_files": true
  },
  "projects_config_path": "config/projects.json",
