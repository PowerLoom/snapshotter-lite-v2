{
  "namespace": "relevant-namespace",
  "core_api": {
    "host": "0.0.0.0",
    "port": 8002
  },
  "instance_id": "account-address",
  "signer_private_key": "signer-account-private-key",
  "slot_id": "slot-id",
  "rpc": {
    "full_nodes": [{
      "url": "https://rpc-url"
    }],
    "archive_nodes": [],
    "force_archive_blocks": 100,
    "retry": 5,
    "request_time_out": 5,
    "skip_epoch_threshold_blocks": 30,
    "polling_interval": 3,
    "connection_limits":{
      "max_connections": 100,
      "max_keepalive_connections": 50,
      "keepalive_expiry": 300
    }
  },
  "simulation_submission_url": "https://snapshotter-api.powerloom.io",
